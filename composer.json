{
    "name": "adrianvm/export-entities",
    "description": "Export Laravel model constants and enums to JavaScript/TypeScript for seamless backend-frontend sharing.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Adrianvm\\ExportEntities\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adrian Mosoiu"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Adrianvm\\ExportEntities\\ExportEntitiesServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.0.2 < 9.0.0",
        "laravel/framework": ">=9.0.0 < 12.0.0"
    }
}
